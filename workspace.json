{
    "version": 1,
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "defaultProject": "cartmc",
    "schematics": {
        "@nrwl/react": {
            "application": {
                "style": "styled-components",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "styled-components"
            },
            "library": {
                "style": "styled-components",
                "linter": "eslint"
            }
        },
        "@nrwl/gatsby": {
            "application": {
                "style": "styled-components"
            }
        }
    },
    "projects": {
        "glowsaurus": {
            "projectType": "application",
            "root": "apps/glowsaurus",
            "sourceRoot": "apps/glowsaurus/src",
            "architect": {
                "build": {
                    "builder": "@nx-plus/docusaurus:browser",
                    "options": {
                        "outputPath": "dist/apps/glowsaurus"
                    }
                },
                "serve": {
                    "builder": "@nx-plus/docusaurus:dev-server",
                    "options": {
                        "port": 3000
                    }
                }
            }
        },
        "layout": {
            "root": "libs/layout",
            "sourceRoot": "libs/layout/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/layout/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/layout/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "controls": {
            "root": "libs/controls",
            "sourceRoot": "libs/controls/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/controls/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/controls/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "cartmc": {
            "root": "apps/cartmc",
            "sourceRoot": "apps/cartmc/src",
            "projectType": "application",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "options": {
                        "outputPath": "dist/apps/cartmc",
                        "index": "apps/cartmc/src/index.html",
                        "main": "apps/cartmc/src/main.tsx",
                        "polyfills": "apps/cartmc/src/polyfills.ts",
                        "tsConfig": "apps/cartmc/tsconfig.app.json",
                        "assets": ["apps/cartmc/src/favicon.ico", "apps/cartmc/src/assets"],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/cartmc/src/environments/environment.ts",
                                    "with": "apps/cartmc/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "cartmc:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "cartmc:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/cartmc/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/cartmc/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "store": {
            "root": "libs/store",
            "sourceRoot": "libs/store/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/web:package",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/libs/store",
                        "tsConfig": "libs/store/tsconfig.lib.json",
                        "project": "libs/store/package.json",
                        "entryFile": "libs/store/src/index.ts",
                        "external": ["react", "react-dom", "react-is", "styled-components"],
                        "babelConfig": "@nrwl/react/plugins/bundle-babel",
                        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
                        "assets": [
                            {
                                "glob": "libs/store/README.md",
                                "input": ".",
                                "output": "."
                            }
                        ]
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/store/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/store/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "emstat": {
            "root": "apps/emstat",
            "sourceRoot": "apps/emstat/src",
            "projectType": "application",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "options": {
                        "outputPath": "dist/apps/emstat",
                        "index": "apps/emstat/src/index.html",
                        "main": "apps/emstat/src/main.tsx",
                        "polyfills": "apps/emstat/src/polyfills.ts",
                        "tsConfig": "apps/emstat/tsconfig.app.json",
                        "assets": ["apps/emstat/src/favicon.ico", "apps/emstat/src/assets"],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/emstat/src/environments/environment.ts",
                                    "with": "apps/emstat/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "emstat:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "emstat:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/emstat/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/emstat/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "glowsite": {
            "root": "apps/glowsite",
            "sourceRoot": "apps/glowsite/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/gatsby:build",
                    "options": {
                        "outputPath": "apps/glowsite/public",
                        "uglify": true,
                        "color": true,
                        "profile": false
                    },
                    "configurations": {
                        "production": {}
                    }
                },
                "serve": {
                    "builder": "@nrwl/gatsby:server",
                    "options": {
                        "buildTarget": "glowsite:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "glowsite:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/glowsite/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/glowsite"],
                    "options": {
                        "jestConfig": "apps/glowsite/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "convmc": {
            "root": "apps/convmc",
            "sourceRoot": "apps/convmc/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/convmc",
                        "index": "apps/convmc/src/index.html",
                        "main": "apps/convmc/src/main.tsx",
                        "polyfills": "apps/convmc/src/polyfills.ts",
                        "tsConfig": "apps/convmc/tsconfig.app.json",
                        "assets": ["apps/convmc/src/favicon.ico", "apps/convmc/src/assets"],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/convmc/src/environments/environment.ts",
                                    "with": "apps/convmc/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "convmc:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "convmc:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/convmc/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/convmc"],
                    "options": {
                        "jestConfig": "apps/convmc/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "dummy": {
            "root": "libs/dummy",
            "sourceRoot": "libs/dummy/src",
            "projectType": "library",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:package",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/libs/dummy",
                        "tsConfig": "libs/dummy/tsconfig.lib.json",
                        "project": "libs/dummy/package.json",
                        "entryFile": "libs/dummy/src/index.ts",
                        "external": ["react", "react-dom", "react-is", "styled-components"],
                        "babelConfig": "@nrwl/react/plugins/bundle-babel",
                        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
                        "assets": [
                            {
                                "glob": "libs/dummy/README.md",
                                "input": ".",
                                "output": "."
                            }
                        ]
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/dummy/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/dummy"],
                    "options": {
                        "jestConfig": "libs/dummy/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    }
}

{
    "version": 1,
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "defaultProject": "cartmc",
    "schematics": {
        "@nrwl/react": {
            "application": {
                "style": "styled-components",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "styled-components"
            },
            "library": {
                "style": "styled-components",
                "linter": "eslint"
            }
        },
        "@nrwl/gatsby": {
            "application": {
                "style": "styled-components"
            }
        }
    },
    "projects": {
        "controls": {
            "root": "libs/controls",
            "sourceRoot": "libs/controls/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/controls/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/controls/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "convmc": {
            "root": "examples/convmc",
            "sourceRoot": "examples/convmc/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/convmc",
                        "index": "examples/convmc/src/index.html",
                        "main": "examples/convmc/src/main.tsx",
                        "tsConfig": "examples/convmc/tsconfig.json",
                        "assets": [
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "convmc:build"
                    }
                }
            }
        },
        "generic": {
            "root": "examples/generic",
            "sourceRoot": "examples/generic/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/generic",
                        "index": "examples/generic/src/index.html",
                        "main": "examples/generic/src/main.tsx",
                        "tsConfig": "examples/generic/tsconfig.json",
                        "assets": [
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "generic:build",
                        "hmr": true
                    }
                }
            }
        },
        "oscillating-move": {
            "root": "examples/oscillating-move",
            "sourceRoot": "examples/oscillating-move/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/oscillating-move",
                        "index": "examples/oscillating-move/src/index.html",
                        "main": "examples/oscillating-move/src/main.tsx",
                        "tsConfig": "examples/oscillating-move/tsconfig.json",
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "oscillating-move:build",
                        "hmr": true
                    }
                }
            }
        },
        "set-digital-out": {
            "root": "examples/set-digital-out",
            "sourceRoot": "examples/set-digital-out/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/set-digital-out",
                        "index": "examples/set-digital-out/src/index.html",
                        "main": "examples/set-digital-out/src/main.tsx",
                        "tsConfig": "examples/set-digital-out/tsconfig.json",
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "set-digital-out:build",
                        "hmr": true
                    }
                }
            }
        },
        "staubli": {
            "root": "examples/staubli",
            "sourceRoot": "examples/staubli/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/staubli",
                        "index": "examples/staubli/src/index.html",
                        "main": "examples/staubli/src/main.tsx",
                        "tsConfig": "examples/staubli/tsconfig.json",
                        "assets": [
                            "examples/staubli/src/assets"
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "staubli:build",
                        "hmr": true
                    }
                }
            }
        },
        "igus": {
            "root": "examples/igus",
            "sourceRoot": "examples/igus/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/igus",
                        "index": "examples/igus/src/index.html",
                        "main": "examples/igus/src/main.tsx",
                        "tsConfig": "examples/igus/tsconfig.json",
                        "assets": [
                            "examples/igus/src/assets"
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "igus:build",
                        "hmr": true
                    }
                }
            }
        },
        "store": {
            "root": "libs/store",
            "sourceRoot": "libs/store/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/web:package",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/libs/store",
                        "tsConfig": "libs/store/tsconfig.lib.json",
                        "project": "libs/store/package.json",
                        "entryFile": "libs/store/src/index.ts",
                        "external": [
                            "react",
                            "react-dom",
                            "react-is",
                            "styled-components"
                        ],
                        "babelConfig": "@nrwl/react/plugins/bundle-babel",
                        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
                        "assets": [
                            {
                                "glob": "libs/store/README.md",
                                "input": ".",
                                "output": "."
                            }
                        ]
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/store/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/store/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    }
}

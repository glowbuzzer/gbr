{
    "version": 1,
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "defaultProject": "cartmc",
    "schematics": {
        "@nrwl/react": {
            "application": {
                "style": "styled-components",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "styled-components"
            },
            "library": {
                "style": "styled-components",
                "linter": "eslint"
            }
        },
        "@nrwl/gatsby": {
            "application": {
                "style": "styled-components"
            }
        }
    },
    "projects": {
        "controls": {
            "root": "libs/controls",
            "sourceRoot": "libs/controls/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/controls/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/controls/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "convmc": {
            "root": "examples/convmc",
            "sourceRoot": "examples/convmc/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/convmc",
                        "index": "examples/convmc/src/index.html",
                        "main": "examples/convmc/src/main.tsx",
                        "polyfills": "examples/convmc/src/polyfills.ts",
                        "tsConfig": "examples/convmc/tsconfig.app.json",
                        "assets": [
                            "examples/convmc/src/favicon.ico",
                            "examples/convmc/src/assets"
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "examples/convmc/src/environments/environment.ts",
                                    "with": "examples/convmc/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "convmc:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "convmc:build:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "examples/convmc/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": [
                        "coverage/examples/convmc"
                    ],
                    "options": {
                        "jestConfig": "examples/convmc/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "generic": {
            "root": "examples/generic",
            "sourceRoot": "examples/generic/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/generic",
                        "index": "examples/generic/src/index.html",
                        "main": "examples/generic/src/main.tsx",
                        "polyfills": "examples/generic/src/polyfills.ts",
                        "tsConfig": "examples/generic/tsconfig.app.json",
                        "assets": [
                            "examples/generic/src/favicon.ico",
                            "examples/generic/src/assets"
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "examples/generic/src/environments/environment.ts",
                                    "with": "examples/generic/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "generic:build",
                        "hmr": true
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "generic:build:production",
                            "hmr": false
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "examples/generic/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": [
                        "coverage/examples/generic"
                    ],
                    "options": {
                        "jestConfig": "examples/generic/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "layout": {
            "root": "libs/layout",
            "sourceRoot": "libs/layout/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/layout/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/layout/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "oscillating-move": {
            "root": "examples/oscillating-move",
            "sourceRoot": "examples/oscillating-move/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/oscillating-move",
                        "index": "examples/oscillating-move/src/index.html",
                        "main": "examples/oscillating-move/src/main.tsx",
                        "tsConfig": "examples/oscillating-move/tsconfig.json",
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "oscillating-move:build",
                        "hmr": true
                    }
                }
            }
        },
        "set-digital-out": {
            "root": "examples/set-digital-out",
            "sourceRoot": "examples/set-digital-out/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/set-digital-out",
                        "index": "examples/set-digital-out/src/index.html",
                        "main": "examples/set-digital-out/src/main.tsx",
                        "tsConfig": "examples/set-digital-out/tsconfig.json",
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "set-digital-out:build",
                        "hmr": true
                    }
                }
            }
        },
        "staubli": {
            "root": "examples/staubli",
            "sourceRoot": "examples/staubli/src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@nrwl/web:build",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/examples/staubli",
                        "index": "examples/staubli/src/index.html",
                        "main": "examples/staubli/src/main.tsx",
                        "tsConfig": "examples/staubli/tsconfig.json",
                        "assets": [
                            "examples/staubli/src/assets"
                        ],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    }
                },
                "serve": {
                    "builder": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "staubli:build",
                        "hmr": true
                    }
                }
            }
        },
        "store": {
            "root": "libs/store",
            "sourceRoot": "libs/store/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/web:package",
                    "outputs": [
                        "{options.outputPath}"
                    ],
                    "options": {
                        "outputPath": "dist/libs/store",
                        "tsConfig": "libs/store/tsconfig.lib.json",
                        "project": "libs/store/package.json",
                        "entryFile": "libs/store/src/index.ts",
                        "external": [
                            "react",
                            "react-dom",
                            "react-is",
                            "styled-components"
                        ],
                        "babelConfig": "@nrwl/react/plugins/bundle-babel",
                        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
                        "assets": [
                            {
                                "glob": "libs/store/README.md",
                                "input": ".",
                                "output": "."
                            }
                        ]
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/store/**/*.{ts,tsx,js,jsx}"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/store/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    }
}
